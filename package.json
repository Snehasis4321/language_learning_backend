{
  "name": "language-learning-backend",
  "version": "1.0.0",
  "description": "Backend for AI-powered language learning application with real-time voice conversation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:agent": "tsx watch src/agent-worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:agent": "node dist/agent-worker.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "language-learning",
    "ai",
    "voice",
    "livekit",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@livekit/agents": "^1.0.7",
    "@livekit/agents-plugin-openai": "^1.0.7",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.1.0",
    "livekit-client": "^2.0.0",
    "livekit-server-sdk": "^2.0.0",
    "pg": "^8.16.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
